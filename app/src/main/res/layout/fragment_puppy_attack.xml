<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".PuppyAttackFragment">

    <!-- TODO: Update blank fragment layout -->

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="0dp"
        android:orientation="vertical">

    <!-- Toolbar for the menu bar -->
    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbarToolbar"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:contentInsetStart="0dp" style="@style/groupStylewhite_A700">
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearColumnmenu"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:layout_width="match_parent" android:orientation="vertical">
            <LinearLayout android:id="@+id/linearContents"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_16pxh"
                android:layout_marginStart="19dp"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageMenu"
                    android:layout_height="@dimen/_12pxh"
                    android:layout_marginBottom="7dp"
                    android:layout_marginTop="6dp"
                    android:layout_width="@dimen/_18pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_menu"
                    tools:ignore="ContentDescription"
                    tools:src="@drawable/img_menu"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtTitle"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="35dp"
                    android:layout_marginTop="1dp"
                    android:layout_weight="0.63"
                    android:layout_width="0dp"
                    android:letterSpacing="0.01"
                    android:lineSpacingExtra="@dimen/_4pxh"
                    android:singleLine="true"
                    android:text="Puppy Attack"
                    style="@style/txtRobotomedium20"
                    tools:text="Puppy Attack"/>
                <ImageView android:id="@+id/imageUser"
                    android:layout_height="@dimen/_20pxh"
                    android:layout_marginBottom="@dimen/_3pxv"
                    android:layout_marginStart="37dp"
                    android:layout_marginTop="2dp"
                    android:layout_width="@dimen/_20pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_user"
                    tools:ignore="ContentDescription"
                    tools:src="@drawable/img_user"/>
                <ImageView android:id="@+id/imageNotification"
                    android:layout_height="19dp"
                    android:layout_marginBottom="@dimen/_3pxv"
                    android:layout_marginStart="24dp"
                    android:layout_marginTop="2dp"
                    android:layout_width="@dimen/_16pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_notification"
                    tools:ignore="ContentDescription"
                    tools:src="@drawable/img_notification"/>
                <ImageView android:id="@+id/imageSearch"
                    android:layout_height="@dimen/_17pxh"
                    android:layout_marginBottom="5dp"
                    android:layout_marginEnd="@dimen/_3pxh"
                    android:layout_marginStart="31dp"
                    android:layout_marginTop="@dimen/_3pxv"
                    android:layout_width="@dimen/_17pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_search"
                    tools:ignore="ContentDescription"
                    tools:src="@drawable/img_search"/>
            </LinearLayout>
            <View android:background="@color/black_900"
                android:id="@+id/lineBorderbottom"
                android:layout_gravity="start"
                android:layout_height="1dp"
                android:layout_marginTop="14dp"
                android:layout_width="360dp"/>
        </LinearLayout>
    </androidx.appcompat.widget.Toolbar>


    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:padding="0dp"
        android:spacing="0dp"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="0dp">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="0dp"
        android:orientation="vertical">

        <LinearLayout android:gravity="start"
            android:id="@+id/linearPuppy"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_10pxv"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylewhite_A700">
            <ImageView android:gravity="center_horizontal"
                android:id="@+id/imageRectangleNineteen"
                android:layout_height="@dimen/_188pxh"
                android:layout_marginEnd="@dimen/_15pxh"
                android:layout_marginStart="@dimen/_15pxh"
                android:layout_width="match_parent"
                android:scaleType="fitXY"
                android:src="@drawable/img_rectangle19"
                tools:ignore="ContentDescription"
                tools:src="@drawable/img_rectangle19"/>
            <ImageView android:gravity="center_horizontal"
                android:id="@+id/imageRectangleTwenty"
                android:layout_height="@dimen/_188pxh"
                android:layout_marginEnd="@dimen/_15pxh"
                android:layout_marginStart="@dimen/_15pxh"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_width="match_parent"
                android:scaleType="fitXY"
                android:src="@drawable/img_rectangle20"
                tools:ignore="ContentDescription"
                tools:src=""/>
            <ImageView android:gravity="center_horizontal"
                android:id="@+id/imageRectangleTwentyOne"
                android:layout_height="@dimen/_188pxh"
                android:layout_marginEnd="@dimen/_14pxh"
                android:layout_marginStart="@dimen/_15pxh"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_width="match_parent"
                android:scaleType="fitXY"
                android:src="@drawable/img_rectangle21"
                tools:ignore="ContentDescription"
                tools:src="@drawable/img_rectangle21"/>
            <FrameLayout android:id="@+id/frameStackrectangletwentytwo"
                android:layout_height="@dimen/_188pxv"
                android:layout_marginTop="@dimen/_10pxv" android:layout_width="match_parent">
                <ImageView android:id="@+id/imageRectangleTwentyTwo"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_188pxh"
                    android:layout_marginEnd="@dimen/_14pxh"
                    android:layout_marginStart="@dimen/_14pxh"
                    android:layout_width="match_parent"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_rectangle22"
                    tools:ignore="ContentDescription"
                    tools:src="@drawable/img_rectangle22"/>
            </FrameLayout>
        </LinearLayout>


    </LinearLayout>

    </ScrollView>

    </LinearLayout>
</FrameLayout>